}

html {
  font-family: 'MyriadPro';
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
}
* {
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  margin: 0px;
  padding: 0px;
}
*:before,
*:after {
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
body, html{
    height: 100%;
}
body {
  margin: 0;
}
html{
    display: block;
}
body,
button,
input,
select,
textarea {
  color: #555555;
   font-size:18px;
   font-family: 'MyriadPro';
